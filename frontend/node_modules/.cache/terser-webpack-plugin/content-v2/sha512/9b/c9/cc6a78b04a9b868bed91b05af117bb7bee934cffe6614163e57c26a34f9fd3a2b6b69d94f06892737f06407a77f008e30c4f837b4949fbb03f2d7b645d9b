 you.\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"h4\",{children:\"Contact\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"a\",{href:\"https://www.instagram.com/codedharsh75/\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{className:\"about-content\",children:\"Instagram\"})}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"a\",{href:\"https://www.youtube.com/channel/UCiD7kslR7lKSaPGSQ-heOWg\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{className:\"about-content\",children:\"Youtube\"})}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"a\",{href:\"https://github.com/harshbhatt7585\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{className:\"about-content\",children:\"GitHub\"})})]})]})]});}\n// EXTERNAL MODULE: ./src/pages/Tutorials/Tutorials.css\nvar Tutorials = __webpack_require__(349);\n\n// CONCATENATED MODULE: ./src/pages/Tutorials/Tutorials.js\nfunction Tutorials_Tutorials(){return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"tutorials-container\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"h1\",{className:\"tutorials-heading\",children:\"Basic Tutorials\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"tutorials-content-container\",children:tutorials.map(function(tutorial){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{className:\"tutorials-content\",children:tutorial});})}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"h1\",{className:\"tutorials-heading\",children:\"Camera Not Working?\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"tutorials-content-container\",children:fixCamera.map(function(points){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{className:\"tutorials-content\",children:points});})})]});}\n// EXTERNAL MODULE: ./src/App.css\nvar App = __webpack_require__(350);\n\n// CONCATENATED MODULE: ./src/App.js\nfunction App_App(){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(react_router_dom[\"a\" /* BrowserRouter */],{children:/*#__PURE__*